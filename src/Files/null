<script type="text/javascript">
//<![CDATA[
jQuery(document).ready(function ($) {
(function($) {
	var check = 0;
    $(function() {
        var jcarousel = $('.jcarousel');
        jcarousel
            .on('jcarousel:reload jcarousel:create', function () {
                var carousel = $(this),
				checkContent = '1';
				tag_id = carousel.parent().attr('id');
				checkContent = $('#'+tag_id).attr('data-checkcontent');
                width = carousel.innerWidth();	
				height = parseInt(carousel.find('.img-item-sj-coolslider img').css('height'));	
                carousel.jcarousel('items').css('width', Math.ceil(width) + 'px'); 
				if(checkContent == 1){
				if(width > 478){
				$('#sjcoolslider_237 .jcarousel-control-prev').mouseover(function(){
					$('#sjcoolslider_237 .jcarousel-control-prev').css('border','1px solid red');
					$('#sjcoolslider_237 .div-img-left .jcarousel-control-prev-btn').css('background','red');
					$('#sjcoolslider_237 .div-img-left .jcarousel-control-prev-btn').css('color','#fff');
				});
				
				$('#sjcoolslider_237 .jcarousel-control-next').mouseover(function(){
					$('#sjcoolslider_237 .jcarousel-control-next').css('border','1px solid red');
					$('#sjcoolslider_237 .div-img-right .jcarousel-control-next-btn').css('background','red');
					$('#sjcoolslider_237 .div-img-right .jcarousel-control-next-btn').css('color','#fff');
				});
				
				$('#sjcoolslider_237 .jcarousel-control-prev').mouseout(function(){
					$('#sjcoolslider_237 .jcarousel-control-prev').css('border','1px solid #fff');
					$('#sjcoolslider_237 .div-img-left .jcarousel-control-prev-btn').css('background','#fff');
					$('#sjcoolslider_237 .div-img-left .jcarousel-control-prev-btn').css('color','#000');
				});
				
				$('#sjcoolslider_237 .jcarousel-control-next').mouseout(function(){
					$('#sjcoolslider_237 .jcarousel-control-next').css('border','1px solid #fff');
					$('#sjcoolslider_237 .div-img-right .jcarousel-control-next-btn').css('background','#fff');
					$('#sjcoolslider_237 .div-img-right .jcarousel-control-next-btn').css('color','#000');
				});
					var width_img = parseInt((width*67)/100);
					var width_content = width - width_img - 60;
					carousel.jcarousel('items').find('.img-item-sj-coolslider').css('width',width_img-3);
					carousel.jcarousel('items').find('.content-item-sj-coolslider').css('width',width_content);
					carousel.jcarousel('items').find('.content-item-sj-coolslider').css('position','absolute');
					$('#'+tag_id+' .jcarousel-control-next').css('left',width_img-74);
					$('#'+tag_id+' .div-img-right').css('left',width_img-74);
					carousel.jcarousel('items').find('iframe,video').attr('width',width_img-3);
					carousel.jcarousel('items').find('iframe,video').attr('height',height);
					$('#'+tag_id).find('.jcarousel-control-next-img,.jcarousel-control-prev-img').css('display','block');
				}else{
					$('#sjcoolslider_237 .div-img-left').css('border','none');
					$('#sjcoolslider_237 .div-img-right').css('border','none');
					$('#sjcoolslider_237 .jcarousel-control-prev').css('border','none');
					$('#sjcoolslider_237 .jcarousel-control-next').css('border','none');
					$('#sjcoolslider_237 .jcarousel-control-prev').mouseover(function(){
						$('#sjcoolslider_237 .jcarousel-control-prev').css('border','none');
						$('#sjcoolslider_237 .div-img-left .jcarousel-control-prev-btn').css('background','red');
						$('#sjcoolslider_237 .div-img-left .jcarousel-control-prev-btn').css('color','#fff');
					});
					
					$('#sjcoolslider_237 .jcarousel-control-next').mouseover(function(){
						$('#sjcoolslider_237 .jcarousel-control-next').css('border','none');
						$('#sjcoolslider_237 .div-img-right .jcarousel-control-next-btn').css('background','red');
						$('#sjcoolslider_237 .div-img-right .jcarousel-control-next-btn').css('color','#fff');
					});
					
					$('#sjcoolslider_237 .jcarousel-control-prev').mouseout(function(){
						$('#sjcoolslider_237 .jcarousel-control-prev').css('border','none');
						$('#sjcoolslider_237 .div-img-left .jcarousel-control-prev-btn').css('background','#fff');
						$('#sjcoolslider_237 .div-img-left .jcarousel-control-prev-btn').css('color','#000');
					});
					
					$('#sjcoolslider_237 .jcarousel-control-next').mouseout(function(){
						$('#sjcoolslider_237 .jcarousel-control-next').css('border','none');
						$('#sjcoolslider_237 .div-img-right .jcarousel-control-next-btn').css('background','#fff');
						$('#sjcoolslider_237 .div-img-right .jcarousel-control-next-btn').css('color','#000');
					});
					
					carousel.jcarousel('items').find('.img-item-sj-coolslider').css('width',width);
					carousel.jcarousel('items').find('.content-item-sj-coolslider').css('width',width-60);
					carousel.jcarousel('items').find('.content-item-sj-coolslider').css('position','relative');
					$('#'+tag_id+' .jcarousel-control-next').css('left',width-74);
					$('#'+tag_id+' .div-img-right').css('left',width-74);
					carousel.jcarousel('items').find('iframe,video').attr('width',width);
					carousel.jcarousel('items').find('iframe,video').attr('height',height);
					$('#'+tag_id).find('.jcarousel-control-prev,.div-img-right').css('border','none');
					$('#'+tag_id).find('.jcarousel-control-next-img,.jcarousel-control-prev-img').css('display','none');
				}
				}else{
					carousel.jcarousel('items').find('.img-item-sj-coolslider').css('width',width);
					carousel.jcarousel('items').find('iframe,video').attr('width',width);
					carousel.jcarousel('items').find('iframe,video').attr('height',height);
					$('#'+tag_id+' .jcarousel-control-next').css('left',width-74);
					$('#'+tag_id+' .div-img-right').css('left',width-74);
				}	
            })
            .jcarousel({
                wrap: 'circular'
            });
			
			$('.jcarousel-control-prev')
            .jcarouselControl({
                target: '-=1',
            });

			$('.jcarousel-control-next')
            .jcarouselControl({
                target: '+=1',
            });	
			
		var stopHover = 0;
		$('.jcarousel').jcarousel({
			animation: {
				duration: 400,
				easing:   'linear',
				complete: function() {
				}
			}
		});
		
		$('#sjcoolslider_237 .jcarousel').eq(0).on('jcarousel:visibleout', 'li', function(event, carousel) {
			if($('#sjcoolslider_237').attr('data-effect') == 'fade'){
			$('#sjcoolslider_237 .jcarousel').eq(0).find('ul li').animate({opacity:"1"},500);
			}
		});
		
		$('#sjcoolslider_237 .jcarousel').eq(0).on('jcarousel:visiblein', 'li', function(event, carousel) {		
			if($('#sjcoolslider_237').attr('data-effect') == 'fade'){
				$('#sjcoolslider_237 .jcarousel').eq(0).find('ul li').animate({opacity:"0"});
			}
		});
		
		$('#sjcoolslider_237 .jcarousel').find('ul li').animate({opacity:"1"},1);
		
		var checkPlay = '1';
		if(checkPlay == 1){

		$('#sjcoolslider_237 .jcarousel').jcarouselAutoscroll({
			interval: 5000		});
		
		stopHover = 'hover';
		}
			if(stopHover != 0){
				$('#sjcoolslider_237 .jcarousel').hover(function() {
				$('#sjcoolslider_237 .jcarousel').jcarouselAutoscroll('stop');
				}, function() {
				$('#sjcoolslider_237 .jcarousel').jcarouselAutoscroll('start');
				});
			}
    });
	$('#sjcoolslider_237').each(function(){
		var $this = $(this);
							$this.touchSwipeLeft(function(){
						$this.find('.jcarousel').jcarousel('scroll', '-=1');
						}
					);
					$this.touchSwipeRight(function(){
						$this.find('.jcarousel').jcarousel('scroll', '+=1');
						}
					);
			
	});
	
	
})(jQuery);
	
});
//]]>
</script>
